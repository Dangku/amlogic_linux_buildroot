config BR2_PACKAGE_GST_PLUGIN_AMLVENC
	bool "gstream1 plugin amlogic x264/x265 encoder"
	help
	  gstreamer 1.0 plugins
	select BR2_PACKAGE_LIBVPCODEC if !BR2_PACKAGE_GST_PLUGIN_AMLVENC_USE_MULTIENC
	select BR2_PACKAGE_LIBVPHEVCODEC if !BR2_PACKAGE_GST_PLUGIN_AMLVENC_USE_MULTIENC
	select BR2_PACKAGE_LIBMULTIENC if BR2_PACKAGE_GST_PLUGIN_AMLVENC_USE_MULTIENC

if BR2_PACKAGE_GST_PLUGIN_AMLVENC
config BR2_PACKAGE_GST_PLUGIN_AMLVENC_USE_MULTIENC
	bool "venc plugin with multi-encoder library"
	default n
	help
		check if use multi-encoder library
endif
