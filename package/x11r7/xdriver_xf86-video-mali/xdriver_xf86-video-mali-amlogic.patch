+int aml_disp_close(aml_disp_t *ctx)
+{
+    if (ctx->fd_disp >= 0) {
+        /* disable cursor */
+        if (ctx->cursor_enabled)
+            aml_hw_cursor_hide(ctx);
+
+        close(ctx->fd_disp);
+        ctx->fd_disp = -1;
+        free(ctx);
+    }
+    return 0;
+}
